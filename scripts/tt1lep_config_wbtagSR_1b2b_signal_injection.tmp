Systematic: "Lumi"
  Title: "Luminosity"
  Type: OVERALL
  OverallUp:    0.017
  OverallDown: -0.017
  Samples: "Zprime*","tt","singletop","diboson","wjets","zjets"
  Category: Instrumental

Systematic: "pileup"
  Title: "pileup modeling"
  Type: HISTO
  HistoNameSufUp: "pileupSF__1up"
  HistoNameSufDown: "pileupSF__1down"
