  Samples: "Zprime*","tt","singletop","diboson","wjets","zjets"
  Symmetrisation: TWOSIDED
  Category: Instrumental

Systematic: "jvt";
  Title: "JVT"
  Type: HISTO
  HistoNameSufUp: "jvtSF__1up"
  HistoNameSufDown: "jvtSF__1down";
  Samples: "Zprime*","tt","singletop","diboson","wjets","zjets"
  Symmetrisation: TWOSIDED
  % Smoothing: 40
  Category: Instrumental

