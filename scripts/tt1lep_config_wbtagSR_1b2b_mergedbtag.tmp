Job: "ttRes1L"
  Label: "t#bar{t} 1-lepton Search"
  AtlasLabel: Internal
  CmeLabel: "13 TeV"
  #LumiLabel: "36.2 fb^{-1}"
  %LumiLabel: "58.5 fb^{-1}"
  %Lumi:  2.376 %139.0/58.5
  LumiLabel: "139 fb^{-1}"
  POI: "mu_Zprime"
  ReadFrom: HIST
  OutputDir: "OUTPUTDIR/"
  HistoPath: "INPUTDIR/"
  DebugLevel: 2
  SystControlPlots: TRUE
  SystErrorBars: TRUE
  UseGammaPulls: TRUE
  RankingPlot: "all"
  RankingMaxNP: 20
  HistoChecks: NOCRASH
  SystPruningShape: 0.005
  SystPruningNorm: 0.005
  CorrelationThreshold: 0.20
  ImageFormat: "pdf"
  PlotOptions: "NOSIG"
  RatioYmax: 1.5
  RatioYmin: 0.6
  StatOnly: STATONLY
  %PieChartCanvasSize: 800,800
  LegendNColumns: 1
  LegendNColumnsSummary: 1
  LegendNColumnsMerge: 1
  SmoothingOption: COMMONTOOLSMOOTHPARABOLIC #MAXVARIATION #TTBARRESONANCE #COMMONTOOLSMOOTHPARABOLIC


% % --------------- %
% % ---  FIT    --- %
% % --------------- %

Fit: "SPLUSB_CRSR_Asimov"
  FitType: SPLUSB
  FitRegion: CRSR
  FitBlind: BLIND
  POIAsimov: 0
  SetRandomInitialNPval: 0.01
  GetGoodnessOfFit: TRUE
  UseMinos: mu_Zprime
  %FitToys: 100

% % --------------- %
% % ---  LIMIT  --- %
% % --------------- %

Limit: "tt1lep_limit"
  LimitType: ASYMPTOTIC
  LimitBlind: BLIND
  ParamName: "ZprimeMass"
  %ParamValue: MASS
  %OutputPrefixName: zprimeMASS
  %POIAsimov:


% %---------------- %
% % -SIGNIFICANCE - %
% %---------------- %

%Significance: "tt1lep_sig"
  %SignificanceBlind: FALSE
  %ParamName: "ZprimeMass"
  %ParamValue: MASS
  %OutputPrefixName: zprimeMASS
  %POIAsimov:


% % --------------- %
% % - NORMFACTORS - %
% % --------------- %

NormFactor: "mu_Zprime"
  Title: "#mu(Z't#bar{t})"
  Nominal: 0
  Min: -5
  Max: 5
  Samples: "Zprime*","KKg*","Grav*"

NormFactor: "ttNorm_boosted"
  Title: "k_{b}(t#bar{t})"
  Nominal: 1
  Min: 0.5
  Max: 1.5
  Samples: tt
  Regions: be1b,be2b,bmu1b,bmu2b


NormFactor: "WjetsNorm_boosted1b"
  Title: "k_{b1b}(W+jets)"
  Nominal: 1
  Min: 0.5
  Max: 1.5
  Samples: wjets
  Regions: be1b,bmu1b

NormFactor: "WjetsNorm_boosted2b"
  Title: "k_{b2b}(W+jets)"
  Nominal: 1
  Min: 0.5
  Max: 1.5
  Samples: wjets
  Regions: be2b,bmu2b

NormFactor: "WjetsNorm_resolved1b"
  Title: "k_{r1b}(W+jets)"
  Nominal: 1
  Min: 0.5
  Max: 1.5
  Samples: wjets
  Regions: re1b,rmu1b

NormFactor: "WjetsNorm_resolved2b"
  Title: "k_{r2b}(W+jets)"
  Nominal: 1
  Min: 0.5
  Max: 1.5
  Samples: wjets
  Regions: re2b,rmu2b



