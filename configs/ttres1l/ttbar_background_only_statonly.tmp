Job: "ttRes1L"
  Label: "t#bar{t} 1-lepton Search"
  AtlasLabel: Internal
  CmeLabel: "13 TeV"
  #LumiLabel: "36.2 fb^{-1}"
  %LumiLabel: "58.5 fb^{-1}"
  %Lumi:  2.376 %139.0/58.5
  LumiLabel: "139 fb^{-1}"
  POI: "mu_Zprime"
  ReadFrom: HIST
  OutputDir: "OUTPUTDIR/"
  HistoPath: "INPUTDIR/"
  InputFolder: "/home/schuya/ttres1lepstat/histograms/ttres1l/tt1lep_config_wbtagSR_1b2b/"
  SplitHistoFiles: TRUE
  DebugLevel: 1
  SystControlPlots: FALSE
  SystErrorBars: TRUE
  UseGammaPulls: TRUE
  RankingPlot: "all"
  RankingMaxNP: 20
  HistoChecks: NOCRASH
  SystPruningShape: 0.005
  SystPruningNorm: 0.005
  CorrelationThreshold: 0.20
  ImageFormat: "pdf","png"
  PlotOptions: "YIELDS","NORMSIG","NORATIO"
  RatioYmax: 1.5
  RatioYmin: 0.6
  StatOnly: STATONLY
  %PieChartCanvasSize: 800,800
  LegendNColumns: 1
  LegendNColumnsSummary: 1
  LegendNColumnsMerge: 1
  SmoothingOption: COMMONTOOLSMOOTHPARABOLIC #MAXVARIATION #TTBARRESONANCE #COMMONTOOLSMOOTHPARABOLIC


% % --------------- %
% % ---  FIT    --- %
% % --------------- %

Fit: "SPLUSB_CRSR_Asimov"
  FitType: SPLUSB
  FitRegion: CRSR
  FitBlind: BLIND
  POIAsimov: FIT_POIASIMOV
  SetRandomInitialNPval: 0.01
  SetRandomInitialNPvalSeed: SEED
  GetGoodnessOfFit: TRUE
  UseMinos: mu_Zprime
  %FitToys: 100

% % --------------- %
% % ---  LIMIT  --- %
% % --------------- %

Limit: "tt1lep_limit"
  LimitType: ASYMPTOTIC
  LimitBlind: BLIND
  OutputPrefixName: SIGNALNAMESIGNALMASS


% %---------------- %
% % -SIGNIFICANCE - %
% %---------------- %

%Significance: "tt1lep_sig"
  %SignificanceBlind: FALSE
  %OutputPrefixName: SIGNALNAMESIGNALMASS
  %POIAsimov:


% % --------------- %
% % - NORMFACTORS - %
% % --------------- %

NormFactor: "mu_Zprime"
  Title: "#mu(Z't#bar{t})"
  Nominal: 0
  Min: -5
  Max: 5
  Samples: "Zprime*"

NormFactor: "ttNorm_boosted"
  Title: "k_{b}(t#bar{t})"
  Nominal: 1
  Min: 0.5
  Max: 1.5
  Samples: tt
  Regions: be1,be2,be3,bmu1,bmu2,bmu3

NormFactor: "WjetsNorm_boosted1"
  Title: "k_{b1}(W+jets)"
  Nominal: 1
  Min: 0.5
  Max: 1.5
  Samples: wjets
  Regions: be1,bmu1

NormFactor: "WjetsNorm_boosted2"
  Title: "k_{b2}(W+jets)"
  Nominal: 1
  Min: 0.5
  Max: 1.5
  Samples: wjets
  Regions: be2,bmu2

NormFactor: "WjetsNorm_boosted3"
  Title: "k_{b3}(W+jets)"
  Nominal: 1
  Min: 0.5
  Max: 1.5
  Samples: wjets
  Regions: be3,bmu3

NormFactor: "WjetsNorm_resolved1"
  Title: "k_{r1}(W+jets)"
  Nominal: 1
  Min: 0.5
  Max: 1.5
  Samples: wjets
  Regions: re1,rmu1

NormFactor: "WjetsNorm_resolved2"
  Title: "k_{r2}(W+jets)"
  Nominal: 1
  Min: 0.5
  Max: 1.5
  Samples: wjets
  Regions: re2,rmu2

NormFactor: "WjetsNorm_resolved3"
  Title: "k_{r3}(W+jets)"
  Nominal: 1
  Min: 0.5
  Max: 1.5
  Samples: wjets
  Regions: re3,rmu3

