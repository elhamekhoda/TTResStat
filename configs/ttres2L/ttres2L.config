% --- non-ttbar background modelling

Systematic:           "ZjetsNorm_LF"            ;"ZjetsNorm_HF"
  Title:              "#it{Z}+light jets norm." ;"#it{Z}+HF jets norm."
  SampleUp:           "zjets_LF_up"             ;"zjets_HF_up"
  Type:               HISTO
  Samples:            "zjets"
  Symmetrisation:     ONESIDED
  SubCategory:        "Z+jets background"
  Category:           "Modelling"

Systematic:           "Zjets_pTll"
  Title:              "#it{Z}+HF jets #it{p}_{T} modelling"
  SampleUp:           "zjets_pTll_up"
  SampleDown:         "zjets_pTll_down"
  Type:               HISTO
  Samples:            "zjets"
  SubCategory:        "Z+jets background"
  Category:           "Modelling"

Systematic:           "FakesNorm_el"
  Title:              "Fakes & NP norm. (e)"
  Type:               HISTO
  Samples:            "fakes","fakes_ttbar"
  WeightSufUp:        "(1.+0.3*isFake_el)"
  Symmetrisation:     ONESIDED
  Smoothing:          40
  SubCategory:        "Fake-lepton background"
  Category:           "Modelling"

Systematic:           "FakesNorm_mu"
  Title:              "Fakes & NP norm. (#mu)"
  Type:               HISTO
  Samples:            "fakes","fakes_ttbar"
  WeightSufUp:        "(1.+0.3*isFake_mu)"
  Symmetrisation:     ONESIDED
  Smoothing:          40
  SubCategory:        "Fake-lepton background"
  Category:           "Modelling"

