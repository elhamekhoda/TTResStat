Job: ttRes1L2L
  AllowWrongRegionSample: TRUE
  AtlasLabel: "Internal"
  CmeLabel: "13 TeV"
  CorrelationThreshold: 0.20
  CustomFunctions: "../NNLOttbar/TTbarNNLOReweighter/macros/RecursiveReweighting_2d_2022.C"
  CustomIncludePaths: "../NNLOttbar/TTbarNNLOReweighter/"
  DebugLevel: 1
  DoSummaryPlot: FALSE
  HistoChecks: NOCRASH
  HistoPath: "INPUTDIR/"
  ImageFormat: "pdf","png"
  InputFolder: "HISTODIR/"
  Label: "t#bar{t} 1-lepton/2-lepton search
  LegendNColumns: 2
  LegendNColumnsMerge: 1
  LegendNColumnsSummary: 1
  LegendX1: 0.5
  Lumi: 139000 % 36100 + 44300 + 58600
  LumiLabel: "139 fb#kern[-0.5]{^{-1}}"
  MCstatThreshold: 0.0001
  MCweight: "weight"
  NtupleName: "nominal"
  NtuplePaths: "INPUTDIR/"
  OutputDir: "OUTPUTDIR/"
  POI: "mu_Zprime"
  PlotOptions: "NOSIG,NORMSIG,POISSONIZE,NOXERR,OPRATIO"
  RankingMaxNP: 20
  RankingPlot: "all"
  RatioYmax: 1.5
  RatioYmin: 0.0
  ReadFrom: HIST
  ReorderNPs: TRUE
  Selection: "(Mllbb/1e3<450 && DeltaEta<1) || (Mllbb/1e3>=450 && Mllbb/1e3<1000 && DeltaEta<1.5) || (Mllbb/1e3>=1000 && Mllbb/1e3<1500 && DeltaEta<2) || (Mllbb/1e3>=1500 && Mllbb/1e3<2500 && DeltaEta<2.5) || Mllbb/1e3>=2500" % adding deltaEta selection here ==> affecting mllbb and deltaPhi
  SmoothingOption: COMMONTOOLSMOOTHPARABOLIC
  SplitHistoFiles: TRUE
  StatOnly: STATONLY
  SystControlPlots: TRUE
  SystErrorBars: TRUE
  SystLarge: 0.90
  SystPruningNorm: 0.005
  SystPruningShape: 0.005
  UseGammaPulls: TRUE

Options: "myOptions"
  SystPlotRatioRange: 20

Fit: fit
  FitBlind: BLIND
  FitRegion: CRSR
  FitType: SPLUSB
  GetGoodnessOfFit: TRUE
  POIAsimov: FIT_POIASIMOV
  SaturatedModel: TRUE
  SetRandomInitialNPval: 0.001
  SetRandomInitialNPvalSeed: SEED
  UseMinos: mu_Zprime

Limit: limit
  LimitBlind: BLIND
  LimitType: ASYMPTOTIC
  OutputPrefixName: SIGNALNAMESIGNALMASS
  ParamName: "ZprimeMass"

Significance: "significance"
  ParamName: "ZprimeMass"
  SignificanceBlind: FALSE

